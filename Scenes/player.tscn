script/source = "extends CharacterBody2D
class_name Player
var speed_x = 70
var speed_y = 40
@export var can_move = true

#areas
@onready var hitbox = $Pivot/Hitbox
@onready var hurtbox = $Hurtbox

#still_time y combos
@onready var still_time = $Still_time
@export var combo_step = 0

## Player script
@onready var animation_tree = $AnimationTree
@onready var animation_player = $AnimationPlayer
@onready var pivot = $Pivot
@onready var playback = animation_tree.get(\"parameters/playback\")

# Audio
@onready var audio_stream_player_kick = $kick
@onready var audio_stream_player_punch = $punch

#vida y barra de vida
@onready var health_bar = $\"../../../CanvasLayer/MarginContainer/HBoxContainer/ProgressBar\"
const Max_health = 200
var health = 200

func on_area_entered(area: Node):
	if area.get_parent().has_method(\"take_damage\"):
		area.get_parent().take_damage('player1')
		
func _ready():
	hitbox.area_entered.connect(on_area_entered)
	health_bar.value = Max_health

#movement
func get_H():
	var input_direction_H = Input.get_axis(\"left\", \"right\")
	if input_direction_H:
		velocity.x = input_direction_H * speed_x
		playback.travel(\"walk\")
		pivot.scale.x = input_direction_H
	else:
		velocity.x = 0

func get_V():
	var input_direction_V = Input.get_axis(\"up\", \"down\")
	if input_direction_V:
		velocity.y = input_direction_V * speed_y
		playback.travel(\"walk\")
	else:
		velocity.y = 0

#attacks
func attacks():
	if can_move:
		if Input.is_action_just_pressed(\"punch\") and combo_step == 0:
			playback.call_deferred(\"travel\", \"punch\")
			audio_stream_player_punch.play()
		elif Input.is_action_just_pressed(\"punch\") and combo_step == 1:
			playback.call_deferred(\"travel\",\"punch2\")
		elif Input.is_action_just_pressed(\"punch\") and combo_step == 2:
			playback.call_deferred(\"travel\",\"punch2\")
		elif Input.is_action_just_pressed(\"kick\"):
			playback.call_deferred(\"travel\", \"kick\")
			audio_stream_player_kick.play()

func take_damage_player(damage):
	health = max(health - damage, 0)
	health_bar.value = health
	if health == 0:
		#ac√° va death
		playback.call_deferred(\"travel\",\"knockdown\")
	else:
		playback.call_deferred(\"travel\",\"hurt\")

func _physics_process(delta):
	attacks()
	if can_move:
		get_H()
		get_V()
	else:
		velocity = Vector2.ZERO
	if velocity.x == 0 and velocity.y == 0 and can_move:
		playback.travel(\"idle\")


	move_and_slide()
"

